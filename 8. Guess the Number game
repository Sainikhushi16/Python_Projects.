import random
n = random.randint(1, 100)
a = -1
guesses = 0 
while(a != n):
    guesses +=1
    a = int(input("guess the number: "))
    if(a >n):
        print("Lower numbr please")
    else:
        print("Higher number please")
        
print(f"You have guessed the number {n} correctly in {guesses} attemps")
